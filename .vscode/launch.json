    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch kbcmd",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/kbcmd",
            "args": [
                "acc",
                "create",
                "ExternalKey=p4",
                "Name=p3",
                "email=p1@p1.com"
            ]
        },
        {
            "name": "Launch RegisterCB",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/kbcmd",
            "args": [
                "--host",
                "127.0.0.1:8080",
                "--user",
                "admin",
                "--password",
                "password",
                "--api_key",
                "bob",
                "--api_secret",
                "lazar",
                "--transport_scheme",
                "http",
                "tenants",
                "registerPushNotificationCallBack",
                "http://host.docker.internal:42722/events"
            ]
        }
    ]
}